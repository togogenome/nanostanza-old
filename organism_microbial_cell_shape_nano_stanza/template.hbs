<!DOCTYPE html>

<html>
  <head>
    <meta charset='utf-8'>
    <title>Organism microbial cell shape</title>
    <link rel="stylesheet" href="/stanza/assets/nanostanza.css" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="/stanza/assets/nanostanza.js"></script>
    {{#if cell_shapes}}
      <script type="text/javascript">
        var NANO_STANZA = {};
        NANO_STANZA.EXTENSION = [];
        NANO_STANZA.EXTENSION.push({label: "Link", functionType: "blank", value: "{{cell_shapes.image_url}}"});
        NANO_STANZA.EXTENSION.push({label: "Open this image", functionType: "blank", value: "{{cell_shapes.image_url}}"});
      </script>
    {{/if}}
  </head>

  <body>
    <h1>Cell shape</h1>
    <div id="main-view">
      {{#if cell_shapes}}
        <figure>
          <img src="{{cell_shapes.image_url}}" alt="{{cell_shapes.label}}" />
          <figcaption>{{cell_shapes.label}}</figcaption>
        </figure>
      {{else}}
        <img src="/stanza/assets/no_data.svg" />
      {{/if}}
    </div>
  </body>
</html>
